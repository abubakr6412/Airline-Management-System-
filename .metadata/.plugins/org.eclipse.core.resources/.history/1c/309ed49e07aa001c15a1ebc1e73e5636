package application;

public class ARS {

	 private static ARS single_instance = null;

	    public static ARS getInstance()
	    {
	        if (single_instance == null)
	            single_instance = new ARS();
	 
	        return single_instance;
	    }	
	
	    
	    
	 //Populating the classes
	    
	
	
}
